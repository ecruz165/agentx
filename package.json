{
  "name": "agentx-skillz",
  "version": "0.1.0",
  "type": "module",
  "description": "Supply chain manager for AI agent configurations",
  "bin": {
    "agentx": "./bin/skillz.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "bin/",
    "dist/",
    "src/scaffolds/"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit/",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.3",
    "handlebars": "^4.7.0",
    "js-yaml": "^4.1.1",
    "octokit": "^4.0.2",
    "ora": "^8.1.0",
    "simple-git": "^3.27.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.3",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
